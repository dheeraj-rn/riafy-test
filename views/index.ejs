<!DOCTYPE html>
<html>

<head>
  <!--Import Google Icon Font-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!--Import materialize.css-->
  <link type="text/css" rel="stylesheet" href="css/materialize.min.css" media="screen,projection" />

  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <style>
    .a1 {
      margin-top: 100px;
      margin-left: 250px;
    }
  </style>
</head>

<body">

  <div class="a1">
    <div class="container center-align">
      <div class="row">
        <div class="col s11">
          <div class="card white">
            <!-- <div class="card-content z-depth-5 black-text"> -->

            <!-- Page Content goes here -->
            <div class="row">
              <% if(typeof instaData !='undefined' ){ %>
              <%  for(let i=0; i<instaData.length; ++i){ %>
              <!-- <div class="col s12 m8 offset-m2 l6 offset-l3"> -->
              <div class="card-panel grey lighten-5 z-depth-1">
                <div class="row valign-wrapper">
                  <div class="col s5">
                    <img src="<%= instaData[i]['low thumbnail'] %>" alt="" class="circle responsive-img">
                    <!-- notice the "circle" class -->
                  </div>
                  <div class="left-align col s12">
                    <span class="black-text">
                      varun.aditya
                    </span>
                  </div>
                  <div class="right-align col s12">
                    <i class="material-icons">more_vert</i>
                  </div>
                </div>
                <div class="grey lighten-5 z-depth-1">
                  <img class="materialboxed" width="650" src="<%= instaData[i]['high thumbnail'] %>">

                </div>
                <div class="left-align col s12">
                  <span class="black-text">
                    <b>varun.aditya</b>
                  </span>
                  <span class="black-text">
                    <%= instaData[i]['title'] %>
                  </span>
                </div>

              </div>
              <% } %>
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="js/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="js/materialize.min.js"></script>
    </body>

</html>